# Google OAuth Configuration
# Get these values from your Google Cloud Console
# https://console.cloud.google.com/apis/credentials

# Google OAuth Client ID for Android
CLIENT_ID=your_android_client_id_here.apps.googleusercontent.com

# Google OAuth Client ID for iOS (if needed)
IOS_CLIENT_ID=your_ios_client_id_here.apps.googleusercontent.com

# Google OAuth Client ID for Web (if needed)
WEB_CLIENT_ID=your_web_client_id_here.apps.googleusercontent.com

# API Base URL
API_BASE_URL=https://your-api-domain.com/api

# Other environment variables
APP_ENV=development
DEBUG_MODE=true
